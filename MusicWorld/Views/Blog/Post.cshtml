@model MusicWorld.Models.Post
@addTagHelper "*,Microsoft.AspNetCore.Mvc.TagHelpers"
@{
    Layout = "_Layout";
}


@await Html.PartialAsync("_Post")

@Html.ActionLink("Back", "Index");
<h6>
    @Html.ActionLink("Edit", "Edit", "Blog", new { id = Model.Id })
    
</h6>
<h6 id="#delete">
    @using (Html.BeginForm("Delete", "Blog", new { id = Model.Id }))
    {
        <input type="submit" value="Delete" />
    }
</h6>
